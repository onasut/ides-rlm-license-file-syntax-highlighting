{
    "name": "ides-rlm-license-file",
    "displayName": "ides rlm license file",
    "description": "Syntax highlighting for Ides RLM license file",
    "version": "0.0.2",
    "publisher": "onasut",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ides-rlm-lic",
            "aliases": ["Ides RLM License File", "rlm-lic"],
            "extensions": [".lic"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ides-rlm-lic",
            "scopeName": "source.rlm",
            "path": "./syntaxes/ides-rlm-lic.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/onasut/ides-rlm-license-file-syntax-highlighting"
    }
}